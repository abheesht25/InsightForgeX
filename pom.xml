<project>
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.apache.maven</groupId>
    <artifactId>maven-artifact</artifactId>
    <packaging>pom</packaging>
    <version>4.0.0-alpha-13</version>
    <name>InsightforgeX</name>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven</groupId>
                <artifactId>maven-artifact</artifactId>
                <version>4.0.0-alpha-13</version>
                <configuration>
                    <xcodeProject>InsightforgeX.xcodeproj</xcodeProject>
                    <xcodeTarget>InsightforgeX</xcodeTarget>
                    <xcodeConfiguration>Release</xcodeConfiguration>
                    <xcodeSdk>iphoneos</xcodeSdk>
                </configuration>
                <extensions>true</extensions>
            </plugin>
        </plugins>
    </build>
</project>
